{
	"manifest_version": 2,
	"name": "Cloud To Spacewater",
	"version": "0.4",
	"description": "Replaces the text 'the cloud' with 'spacewater'.",
	"icons": { "16": "icon-16.png"},
	"short_name": "Cloud-to-Spacewater",
	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

